{
  "manifest_version": 3,
  "name": "Tab Namer",
  "version": "1.3",
  "description": "Renames tabs to: \"<index> <original_name> <hostname>\".",
  "background": {
    "service_worker": "service_worker.js",
    "scripts": ["service_worker.js"]
  },
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["content_script.js"],
      "run_at": "document_start"
    }
  ],
  "content_security_policy": {}
}
